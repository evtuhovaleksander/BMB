<?xml version='1.0' encoding='utf-8'?>
<!DOCTYPE hibernate-mapping PUBLIC
    "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
    "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping>

    <class name="busormAnotaited.BusdriverEntity" table="busdriver" schema="buspark">
        <composite-id mapped="true" class="busormAnotaited.BusdriverEntityPK">
            <key-property name="busId">
                <column name="bus_id" sql-type="int(11)"/>
            </key-property>
            <key-property name="driverId">
                <column name="driver_id" sql-type="int(11)"/>
            </key-property>
        </composite-id>
    </class>
</hibernate-mapping>